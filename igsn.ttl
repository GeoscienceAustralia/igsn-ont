# baseURI: http://pig.geoscience.gov.au/def/igsn
# imports: http://dublincore.org/2012/06/14/dcterms.rdf
# imports: http://pid.geoscience.gov.au/def/voc/igsn-codelists/
# imports: http://purl.org/dc/elements/1.1/
# imports: http://xmlns.com/foaf/spec/index.rdf
# imports: https://www.w3.org/2009/08/skos-reference/skos.rdf

@prefix : <http://pig.geoscience.gov.au/def/igsn#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix dcterms: <http://purl.org/dc/terms/> .
@prefix igsn-codelists: <http://pid.geoscience.gov.au/def/voc/igsn-codelists/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

<http://pig.geoscience.gov.au/def/igsn>
  rdf:type owl:Ontology ;
  dc:creator "Nicholas Car"^^xsd:string ;
  dcterms:created "2016-09-15"^^xsd:string ;
  rdfs:comment "This ontology is an OWL representation of the IGSN descriptive metadata v1.0 described at https://github.com/IGSN/metadata/wiki/IGSN-Descriptive-Metadata-Version-1.0"^^xsd:string ;
  rdfs:label "IGSN metadata ontology"^^xsd:string ;
  owl:imports <http://dublincore.org/2012/06/14/dcterms.rdf> ;
  owl:imports igsn-codelists: ;
  owl:imports dc: ;
  owl:imports <http://xmlns.com/foaf/spec/index.rdf> ;
  owl:imports <https://www.w3.org/2009/08/skos-reference/skos.rdf> ;
  owl:versionInfo "1.0"^^xsd:string ;
.
:AlternateIdentifier
  rdf:type owl:Class ;
  rdfs:label "Alternate Identifier"^^xsd:string ;
.
:CollectionIdentifier
  rdf:type owl:Class ;
  rdfs:label "Collection Identifier"^^xsd:string ;
.
:Resource
  rdf:type owl:Class ;
  rdfs:label "Resource"^^xsd:string ;
.
:alternateIdentifier
  rdf:type rdf:Property ;
  rdfs:domain :Resource ;
  rdfs:label "alternateIdentifier"^^xsd:string ;
.
:collector
  rdf:type owl:AnnotationProperty ;
  rdfs:domain :Resource ;
  rdfs:label "collector"^^xsd:string ;
  rdfs:range xsd:string ;
.
:identifier
  rdf:type rdf:Property ;
  rdfs:domain :AlternateIdentifier ;
  rdfs:domain :CollectionIdentifier ;
  rdfs:domain :Resource ;
  rdfs:label "identifier"^^xsd:string ;
  rdfs:range xsd:string ;
.
:identifierType
  rdf:type owl:ObjectProperty ;
  rdfs:domain :AlternateIdentifier ;
  rdfs:domain :CollectionIdentifier ;
  rdfs:domain :Resource ;
  rdfs:label "identifierType"^^xsd:string ;
  rdfs:range <http://pid.geoscience.gov.au/def/ont/igsn#Identifier> ;
.
:name
  rdf:type rdf:Property ;
  rdfs:domain :Resource ;
  rdfs:label "name"^^xsd:string ;
  rdfs:range xsd:string ;
.
